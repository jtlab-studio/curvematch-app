# API Configuration (update with your production API URL)
VITE_API_URL=https://api.curvematch.com

# Map Configuration
VITE_MAP_DEFAULT_LAT=52.5200
VITE_MAP_DEFAULT_LNG=13.4050
VITE_MAP_DEFAULT_ZOOM=11

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_DEBUG=false

# Map Tile Server (optional - defaults to OpenStreetMap)
VITE_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_TILE_ATTRIBUTION=&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors

# Production-specific settings
VITE_ENABLE_MOCK_DATA=false
VITE_LOG_LEVEL=error